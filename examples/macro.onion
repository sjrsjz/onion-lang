@required stdlib;

@macro A := (
    add(_A, _B)
) : _A + _B;

@A stdlib.io.println(add(1, 2));

@macro combine := (
    _A, _B
) : _A(_B);

@combine stdlib.io.println, "Hello, World!";

@macro interface := (
    _def
) : (struct?, interface => mut _def) -> struct : interface;

@interface A := {
    method => () -> {
        return self.data
    },
};

