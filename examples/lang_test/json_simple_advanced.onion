// 简化的 JSON 高级示例
@required stdlib;

stdlib.io.println("JSON 高级示例开始");

// 测试嵌套对象
nested := {
    "level1": {
        "level2": {
            "value": 42
        }
    }
};

stdlib.io.println("嵌套对象:", nested);
json_result := stdlib.json.stringify_pretty(nested);
stdlib.io.println("JSON 结果:");
stdlib.io.println(json_result);

stdlib.io.println("示例完成");
