@required stdlib;
f := mut stdlib.math.floor;
a := mut 100;
while (a > 0) {
    b := mut 100;
    while (b > 0) {
        c := mut 100;
        while (c > 0) {
            f(4);
            c = c - 1;
        };
        b = b - 1;
    };
    a = a - 1;
};

n := 10000000;
while (n > 0) (
    n := n - 1
);

fib := "n" -> {
    if (n <= 1) {
        return n;
    };
    return this(n - 1) + this(n - 2);
};